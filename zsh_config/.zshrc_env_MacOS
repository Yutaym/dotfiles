export LC_ALL=en_US.UTF-8
export PATH=/usr/local:$PATH
export PATH=~/scripts:$PATH
export PATH=/Library/TeX/texbin:$PATH

export PATH=/Users/yuta/opt/miniconda3:$PATH
export PATH=/Users/yuta/opt/miniconda3/bin:$PATH
export PATH=/Users/yuta/opt/miniconda3/envs:$PATH
export PATH=/Users/yuta/opt/miniconda3/condabin:$PATH


# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/Users/yuta/opt/miniconda3/bin/conda' 'shell.zsh' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/yuta/opt/miniconda3/etc/profile.d/conda.sh" ]; then
        . "/Users/yuta/opt/miniconda3/etc/profile.d/conda.sh"
    else
        export PATH="/Users/yuta/opt/miniconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<
